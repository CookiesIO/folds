{
  "name": "folds",
  "version": "1.5.1",
  "author": "Ajay Bura (ajbura)",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "npm run check:eslint && npm run check:prettier",
    "check:eslint": "eslint src/*",
    "check:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider storybook dev",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint",
    "*": "prettier --ignore-unknown --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cinnyapp/folds.git"
  },
  "peerDependencies": {
    "@vanilla-extract/css": "^1.14.0",
    "@vanilla-extract/recipes": "^0.5.1",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.9.6"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@fontsource/inter": "^4.5.12",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^7.6.8",
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-interactions": "^7.6.8",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/react": "^7.6.8",
    "@storybook/react-vite": "^7.6.8",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vanilla-extract/css": "^1.14.0",
    "@vanilla-extract/recipes": "^0.5.1",
    "@vanilla-extract/vite-plugin": "^3.9.3",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^9.1.3",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "^2.9.6",
    "semantic-release": "^22.0.12",
    "storybook": "^7.6.8",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-dts": "^3.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
